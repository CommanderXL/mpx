<template>
  <view>
    <view bindtap="tap">this is b component</view>
    <slot name="before"></slot>
    <slot></slot>
    --------------------
  </view>
</template>

<script>
import { createComponent } from '@mpxjs/core'

createComponent({
  options: {
    multipleSlots: true
  },
  properties: {
    a: {
      type: String,
      value: 'this is a'
    },
    b: {
      type: Number,
      value: 100
    }
  },
  attached () {
    console.log('-------b', this.a, ',', this.b)
  },
  methods: {
    tap () {
      console.log('tap tap')
      this.triggerEvent('click')
    }
  }
})
</script>

